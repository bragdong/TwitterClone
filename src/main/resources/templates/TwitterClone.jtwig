<!DOCTYPE html>
<html>
<head>

</head>
<body>
<h1>{{title}}</h1>

<h4>{{link1}}&emsp;{{link2}}&emsp;{{link3}}&emsp;{{link4}}</h4>

{% for id in timeline %}
<div>
    <ul>
        <li>{{id}}</li>
    </ul>
</div>
{% endfor %} 

<script>
    var xhr = new XMLHttpRequest();
    xhr.open('POST', '/like');
    xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
    xhr.onload = function() {
    	if (xhr.status === 200) {
			console.log('Request passed');
			window.location.assign("/timeLine");
        } else if (xhr.status !== 200) {
            alert('Request failed.  Returned status of ' + xhr.status);
        }
   		};
    
	function myFunction(id) {
    var body = 'tweet_id=' + encodeURIComponent(id);
    console.log(body);
    xhr.send(body); }
    
</script>
</body>
</html>